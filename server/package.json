{
  "name": "playola",
  "version": "1.2.0",
  "description": "Tools for creating and maintaining internet radio stations",
  "main": "index.js",
  "scripts": {
    "test": "env-cmd -f .env-test mocha --require ts-node/register",
    "test:watch": "env-cmd -f .env-test mocha \"./{,!(node_modules)/**}/*.test.ts\" --watch --recursive --exit",
    "test:debug": "env-cmd -f .env-test mocha \"./{,!(node_modules)/**}/*.test.ts\" --recursive --exit --inspect=0.0.0.0",
    "build": "npx tsc",
    "dev": "nodemon -L server.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/briankeane/playola.git"
  },
  "author": "Playola Radio, Incorporated",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/briankeane/playola/issues"
  },
  "homepage": "https://github.com/briankeane/playola#readme",
  "dependencies": {
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "express": "^4.18.3",
    "express-bearer-token": "^2.4.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.12",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/mocha": "^10.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.28",
    "@types/supertest": "^6.0.2",
    "chai": "^5.1.0",
    "mocha": "^10.3.0",
    "nodemon": "^3.1.0",
    "supertest": "^6.3.4",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  }
}
